{
  "dxt_version": "0.3",
  "name": "@moonbanking/mcp",
  "version": "0.1.6",
  "description": "The official MCP Server for the Moon Banking API",
  "author": {
    "name": "Moon Banking",
    "email": "support+sdk@moonbanking.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonbanking/moonbanking-node.git"
  },
  "homepage": "https://github.com/moonbanking/moonbanking-node/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.moonbanking.com",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "MOON_BANKING_API_KEY": "${user_config.MOON_BANKING_API_KEY}"
      }
    }
  },
  "user_config": {
    "MOON_BANKING_API_KEY": {
      "title": "bearer_token",
      "description": "",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
